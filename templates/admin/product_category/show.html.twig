{% extends 'base.html.twig' %}

{% block title %}ProductCategory{% endblock %}

{% block body %}
    <h1>ProductCategory</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ product_category.id }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ product_category.title }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ product_category.image }}</td>
            </tr>
            <tr>
                <th>Language</th>
                <td>{{ product_category.language }}</td>
            </tr>
            <tr>
                <th>ImageUpdatedAt</th>
                <td>{{ product_category.imageUpdatedAt ? product_category.imageUpdatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_admin_product_category_index') }}">back to list</a>

    <a href="{{ path('app_admin_product_category_edit', {'id': product_category.id}) }}">edit</a>

    {{ include('admin/product_category/_delete_form.html.twig') }}
{% endblock %}

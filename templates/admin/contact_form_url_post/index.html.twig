{% extends 'base.html.twig' %}

{% block title %}ContactFormUrlPost index{% endblock %}

{% block body %}
    <h1>ContactFormUrlPost index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Url</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for contact_form_url_post in contact_form_url_posts %}
            <tr>
                <td>{{ contact_form_url_post.id }}</td>
                <td>{{ contact_form_url_post.url }}</td>
                <td>
                    <a href="{{ path('app_admin_contact_form_url_post_show', {'id': contact_form_url_post.id}) }}">show</a>
                    <a href="{{ path('app_admin_contact_form_url_post_edit', {'id': contact_form_url_post.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_admin_contact_form_url_post_new') }}">Create new</a>
{% endblock %}
